<div class="random-beers-wrap">
    <div class="random-beer-container">
        {{> beerPartial randomBeer}}
        {{#with randomBeer}}
        <h3 class="info">Food Pairing</h3>
        <ul class="food-pairing-list">
            {{#each food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <h3 class="info">Brewers Tips</h3>
        <p>{{brewers_tips}}</p>
        {{/with}}
    </div>
</div>